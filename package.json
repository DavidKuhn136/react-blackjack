{
  "name": "react-blackjack",
  "version": "1.0.0",
  "description": "BlackJack game built with React and Redux",
  "main": "index.js",
  "scripts": {
    "webpack": "node_modules/.bin/webpack",
    "webpack:watch": "npm run webpack -- --watch",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "webpack-dev-server": "node_modules/.bin/webpack-dev-server --mode=development",
    "webpack-dev-server:open": "webpack-dev-server --mode=development --open chrome",
    "webpack-dev-server:prod": "webpack-dev-server --mode=production --open chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidKuhn136/react-blackjack.git"
  },
  "keywords": [
    "react;",
    "redux;",
    "blackjack"
  ],
  "author": "David Kuhn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DavidKuhn136/react-blackjack/issues"
  },
  "homepage": "https://github.com/DavidKuhn136/react-blackjack#readme",
  "dependencies": {
    "immutable": "^3.8.2",
    "npm": "^5.8.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "seedrandom": "^2.4.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.8.3",
    "proxyquire": "^2.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.1.1",
    "sass-loader": "^7.0.1",
    "sinon": "^4.5.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
}
